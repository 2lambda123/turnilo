{
  "compilerOptions": {
    "declaration": false,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "baseUrl": "../node_modules",
    "target": "es5",
    "jsx": "react",
    "lib": ["es6", "es5", "dom"],
    "types": ["cypress", "node"]
  },
  "include": [
    "**/*.ts"
  ]
}
